<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="homepop_item">
        <item name="android:padding">10dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:background">@drawable/menu_item_bg_sel</item>
    </style>

    <style name="menu_icon_size">
        <item name="android:layout_height">@dimen/menu_icon_w_h</item>
        <item name="android:layout_width">@dimen/menu_icon_w_h</item>
    </style>

    <!--<View
            android:layout_width="match_parent"
            android:layout_height="@dimen/hom_item_div"/>-->

    <style name="divid_tyle_m">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/common_divid_m</item>
    </style>

    <style name="divid_line">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1px</item>
        <item name="android:background">@color/divid_gray</item>
    </style>

    <!--android:id="@+id/login_phone"
        android:drawableLeft="@mipmap/phone"
        android:layout_width="match_parent"
        android:hint="@string/phone"
        android:layout_height="wrap_content"-->

    <style name="login_et_style" parent="nomal_b_font">
        <item name="android:layout_marginTop">10dp</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:paddingRight">@dimen/common_pad_m</item>
        <item name="android:paddingLeft">@dimen/common_pad_m</item>
    </style>

    <style name="set_tr_style" parent="common_pad_white_b_h">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:weightSum">1</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:gravity">center_vertical</item>
    </style>

    <style name="set_tv_style" parent="title2_b_font">

        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="set_tv_w_style" parent="set_tv_style">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1</item>
    </style>


    <style name="set_img_style">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">17dp</item>
        <item name="android:scaleType">centerInside</item>
        <item name="android:src">@mipmap/next_icon</item>
    </style>

    <style name="item_img_b_size">
        <item name="android:layout_width">110dp</item>
        <item name="android:layout_height">60dp</item>
    </style>

</resources>